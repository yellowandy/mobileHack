<div class="bizcard" style="background-color: white; height: 100%">
    {{#with model.attributes}}
        <h1>{{name}}</h1>
        <hr>
        <table style="color: black">
            <tr>
                <td>id</td>
                <td>{{id}}</td>
            </tr>
            <tr>
                <td>Created by</td>
                <td>{{created_by_name}}</td>
            </tr>
            <tr>
                <td>City</td>
                <td>{{shipping_address_city}}</td>
            </tr>
        </table>
    {{/with}}
</div>
